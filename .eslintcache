urn sum > 0;\n    }\n\n    purchasingHandler = () => {\n        this.setState({ purchasing: true });\n    }\n\n    purchasingCancleHandler = () => {\n        this.setState({ purchasing: false });\n    }\n    purchasedContinue = () => {\n        this.props.history.push('/checkout');\n    }\n\n\n    componentDidMount() {\n        axios.get('https://react-burger-builder-6e91f-default-rtdb.firebaseio.com/ingredients.json')\n            .then(response => {\n                this.setState({ ingredients: response.data });\n            })\n            .catch(error => {\n                this.setState({ error: true });\n            });\n    }\n\n    render() {\n        let disabledInfo = {\n            ...this.props.ings\n        };\n\n        for (let key in disabledInfo) {\n            disabledInfo[key] = disabledInfo[key] <= 0;\n        }\n\n        let orderSummary = null;\n\n        let burger = this.state.error ? <p> Ingredients can't loaded!</p> : <LoadingSpinner />;\n        if (this.props.ings) {\n            burger = (<Aux>\n                <Burger ingredients={this.props.ings} />\n                <BuildControls\n                    ingredientAddition={this.props.onIngredientAdded}\n                    ingredientDistruction={this.props.onIngredientRemoved}\n                    disabledInfo={disabledInfo}\n                    price={this.props.price}\n                    purchasable={this.updatePurchaseState(this.props.ings)}\n                    ordered={this.purchasingHandler}\n                />\n            </Aux>\n            );\n\n            orderSummary = (<OrderSummary\n                ingredients={this.props.ings}\n                price={this.props.price}\n                cancelPurchased={this.purchasingCancleHandler}\n                continuedPurchase={this.purchasedContinue}\n            />);\n        }\n\n        if (this.state.loading) {\n            orderSummary = <LoadingSpinner />;\n        }\n\n        return (\n            <Aux>\n                <Modal show={this.state.purchasing} modalClosed={this.purchasingCancleHandler}>\n                    {orderSummary}\n                </Modal>\n                {burger}\n            </Aux>\n        )\n    }\n}\n\nconst mapStateToProps = state => {\n    return {\n        ings: state.ingredients,\n        price: state.totalPrice\n    };\n}\n\nconst mapDispatchToProps = dispatch => {\n    return {\n        onIngredientAdded: (ingsName) => dispatch(burgerBuilderActions.addIngredients(ingsName)),\n        onIngredientRemoved: (ingsName) => dispatch(burgerBuilderActions.removeIngredients(ingsName))\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(withErrorHandler(BurgerBuilder, axios));","/Users/sohag/Desktop/React/burger_builder/src/components/Burger/Burger.js",[],"/Users/sohag/Desktop/React/burger_builder/src/components/Burger/BurgerIngredient/BurgerIngredient.js",[],"/Users/sohag/Desktop/React/burger_builder/src/components/BuildControls/BuildControls.js",[],"/Users/sohag/Desktop/React/burger_builder/src/components/BuildControls/BuildControl/BuildControl.js",[],"/Users/sohag/Desktop/React/burger_builder/src/components/UI/Modal/Modal.js",[],"/Users/sohag/Desktop/React/burger_builder/src/components/Burger/OrderSummary/OrderSummary.js",[],"/Users/sohag/Desktop/React/burger_builder/src/hoc/aux/aux.js",[],["152","153"],"/Users/sohag/Desktop/React/burger_builder/src/components/UI/Backdrop/Backdrop.js",[],"/Users/sohag/Desktop/React/burger_builder/src/components/UI/Button/Button.js",[],"/Users/sohag/Desktop/React/burger_builder/src/components/Navigation/Toolbar/Toolbar.js",[],"/Users/sohag/Desktop/React/burger_builder/src/components/Logo/Logo.js",["154"],"import React from 'react';\nimport LogoBurger from '../../assets/images/burger_logo.png';\nimport classes from './Logo.module.css';\n\nconst logo = (props) => (\n    <div className={classes.Logo}>\n        <img src={LogoBurger} />\n    </div>\n);\n\nexport default logo;","/Users/sohag/Desktop/React/burger_